sudo sed â€“i "s/NoDisplay=false/NoDisplay=true/g" /etc/xdg/autostart/*.desktop

FILE="$HOME/.ubunsys/status/HiddenStartupItems.txt"
if [ -f $FILE ]; then
   echo "File $FILE exists."
   echo unShow > $FILE
else
   echo "File $FILE does not exist."
   touch $FILE
   echo unShow > $FILE
fi